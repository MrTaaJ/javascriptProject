{
  "name": "js-template",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint": "^8.x",
    "eslint-config-airbnb-base": "^15.x",
    "eslint-plugin-import": "^2.x",
    "hint": "^6.x",
    "prettier": "^1.19.1",
    "stylelint": "^14.x",
    "stylelint-config-recommended": "^6.x",
    "stylelint-config-standard": "^24.x",
    "stylelint-csstree-validator": "^1.x",
    "stylelint-scss": "^3.x",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "format": "npx prettier  --write **/*.{html,js,json,md,json,yaml,yml,jsx}",
    "format:check": "npx prettier  --check **/*.{html,js,json,md,json,yaml,yml}",
    "lint": "npx stylelint \"**/*.css\" --fix && npx eslint . --fix",
    "lint:check": "npx stylelint \"**/*.css\" && npx eslint .",
    "stylelint": "npx stylelint **/*.css --fix",
    "stylelint:check": "npx stylelint **/*.css",
    "eslint": "npx eslint . --fix",
    "eslint:check": "npx eslint ."
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
